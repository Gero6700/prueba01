{
    "Serilog": {
        "Using": [ "Serilog.Sinks.File" ],
        "MinimumLevel": {
            "Default": "Information"
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/senator-as400-cloud-sync-api_.log",
                    "outputTemplate": "[{Timestamp:dd/MM/yyyy HH:mm:ss.fff zzz} {Level:u3}] {Message}{NewLine:1}{Exception:1}",
                    "rollOnFileSizeLimit": true,
                    "fileSizeLimitBytes": 4194304,
                    "retainedFileCountLimit": 10,
                    "rollingInterval": "Day",
                    "hooks": "Senator.As400.Cloud.Sync.Api.Logging.SerilogHooks::CustomHeaderWriter, Senator.As400.Cloud.Sync.Api"
                }
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithProcessId",
            "WithThreadId"
        ],
        "Properties": {
            "ApplicationName": "Senator AS400 Cloud Sync API"
        }
    },
    "AllowedHosts": "*",
    "MessageQueuing": {
        "HostName": "localhost",
        "Port": 6379
    },
    "HotelChain": {
        "Id": "1",
        "UserName": "integracion@senatorhr.com",
        "Password": ""
    },
    "AvailGooglePubSub": {
        "ProjectId": "dmultichannelwebservice-e09ozd",
        "SubscriptionId": "gcp-topic-avail-sub-order",
        "PullIntervalInMinutes": 1
    },
    "AvailabilitySynchronizerApi": {
        "BaseUrl": "http://localhost:7000",
        "HealthCheckEndpoint": "api/synchronize"
    },
    "BookingSynchronizerApi": {
        "BaseUrl": "http://localhost:7001",
        "HealthCheckEndpoint": "api/synchronize"
    },
    "StaticDataSynchronizerApi": {
        "BaseUrl": "http://localhost:7002",
        "HealthCheckEndpoint": "api/synchronize"
    }
}
